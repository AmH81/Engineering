FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 13.371;

boundaryField
{

    Inlet
    {
        type            fixedValue;
        value           uniform 13.371;
    }
    
    Outlet
    {
        type            inletOutlet;
        value           $internalField;
        inletValue      $internalField;
    }

    Wall
    {
        type            omegaWallFunction;
        value           $internalField;
    }

}


