FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 5.3 0);

boundaryField
{

    Inlet
    {
        type                fixedValue;
        value               uniform (0 5.3 0); //Q=150m^3/h = 0.0416667m^3/s, A=0.00785m^3, V=5.3m/s
    }
    
    Outlet
    {
        type            inletOutlet;
        value           $internalField;
        inletValue      uniform (0 0 0);
    }

    Wall
    {
        type            noSlip;
    }

}

// ************************************************************************* //
